{
  "presets": [
    [
      "es2015",
      {
        "loose": true,
        "modules": false
      }
    ], "stage-0", "react"
  ],
  "plugins": [
    "transform-class-properties",
    "transform-object-assign",
    "transform-object-rest-spread"
  ],
  "env": {
    "test": {
      "plugins": [
        "transform-es2015-modules-commonjs"
      ]
    },
    "production": {
      "plugins": [
        "transform-runtime"
      ]
    }
  }
}
